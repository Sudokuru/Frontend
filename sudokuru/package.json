{
  "name": "sudokuru-frontend",
  "version": "1.0.0",
  "main": "./app/App.tsx",
  "scripts": {
    "start": "expo start --clear",
    "start:atlas": "cross-env EXPO_ATLAS=true expo start --clear",
    "start:atlas:prod": "cross-env EXPO_ATLAS=true expo start --clear --no-dev",
    "start:tunnel": "expo start --tunnel --clear",
    "android": "expo start --android --clear",
    "ios": "expo start --ios --clear",
    "web": "npx expo start --web --clear",
    "web:dev": "cross-env ENV=development npx expo start --web --clear",
    "web:prod": "npx expo start --web --clear --no-dev --minify",
    "web:serve:prod": "npm run build:metro-web && npx serve dist -l 8081",
    "build:metro-web": "npx expo export --platform web --dump-sourcemap",
    "tauri:run": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "expo:check": "npx expo install --check",
    "tsc": "npx tsc --noEmit"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo/metro-runtime": "6.1.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "7.8.4",
    "@react-navigation/drawer": "7.7.2",
    "@react-navigation/native": "7.1.19",
    "@react-navigation/stack": "7.6.3",
    "@react-navigation/elements": "2.8.1",
    "expo": "54.0.23",
    "expo-linear-gradient": "15.0.7",
    "expo-status-bar": "3.0.8",
    "polished": "^4.2.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "2.28.0",
    "react-native-paper": "5.14.5",
    "react-native-reanimated": "4.1.3",
    "react-native-safe-area-context": "5.6.2",
    "react-native-screens": "4.16.0",
    "react-native-web": "0.21.0",
    "sudokuru": "3.4.0",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@tauri-apps/cli": "^2.9.6",
    "@types/node": "^20.11.17",
    "@types/react": "19.1.10",
    "@types/react-native-web": "0.19.0",
    "babel-plugin-istanbul": "^6.1.1",
    "cross-env": "^7.0.3",
    "expo-atlas": "0.4.0",
    "expo-doctor": "1.17.7",
    "metro": "0.83.1"
  },
  "private": true
}
